{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "ADF-for-ete-project"
		},
		"Git_file_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'Git_file'"
		},
		"Git_to_gen_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'Git_to_gen'"
		},
		"Git_file_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://github.com/raavibharath/ADF"
		},
		"Git_file_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "raavibharat"
		},
		"Git_to_gen_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://deprojecttorage.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ADF-for-ete-project_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"Git_file_password": {
						"value": "[parameters('Git_file_password')]"
					},
					"Git_to_gen_accountKey": {
						"value": "[parameters('Git_to_gen_accountKey')]"
					},
					"Git_file_properties_typeProperties_url": {
						"value": "[parameters('Git_file_properties_typeProperties_url')]"
					},
					"Git_file_properties_typeProperties_userName": {
						"value": "[parameters('Git_file_properties_typeProperties_userName')]"
					},
					"Git_to_gen_properties_typeProperties_url": {
						"value": "[parameters('Git_to_gen_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}